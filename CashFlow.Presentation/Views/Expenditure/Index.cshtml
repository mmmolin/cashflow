@model List<CashFlow.Core.Entities.Expenditure>
@*
    
*@
@{
    <h1>Works!</h1>
    <a asp-action="Create">+</a>
    <table>
        <tr>
            <th>Id</th>
            <th>Description</th>
            <th>Amount</th>
        </tr>
        @foreach(var expenditure in Model)
        {
            <tr>
                <td>@expenditure.Id</td>
                <td>@expenditure.Description</td>
                <td>@expenditure.Amount</td>
                <td><a asp-action="Update" asp-route-id="@expenditure.Id">#</a></td>
            </tr>
        }
    </table>
}
